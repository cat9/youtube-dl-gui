{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Main Process", // 配置方案名字, VSCode左下角可以切换配置方案
        "type": "node",
        "request": "launch",
       "cwd":"${workspaceRoot}",
       "runtimeExecutable":"${workspaceRoot}/node_modules/.bin/electron",
       "windows":{
           "runtimeExecutable":"${workspaceRoot}/node_modules/.bin/electron.cmd"
       },
       "args":[".","--dev"],
        "outputCapture": "std",
      },
    ]
  }